APP_TITLE=Faucet Maintenance Prototype

# Static users (username:role:password) comma-separated
USERS=admin:admin:admin123,tech:technician:tech123

# File upload constraints
MAX_FILE_SIZE_MB=10
ALLOWED_TYPES=jpg,jpeg,png

# Edit window in minutes
EDIT_WINDOW_MINUTES=60

# SQLite file name (inside container/app directory)
DB_PATH=prototype.db

# Google APIs
GOOGLE_MAPS_API_KEY=XXXXXXXXXXXXXXXXXXXXXXXXXXXX-n4NSHA
GOOGLE_APPLICATION_CREDENTIALS=/app/keys/service-account.json
USE_VISION=true

# Streamlit config
STREAMLIT_SERVER_PORT=8501
STREAMLIT_ENABLE_XSRF_PROTECTION=true

# Optional: initial map center (Cairo by default)
DEFAULT_LAT=30.0444
DEFAULT_LON=31.2357
